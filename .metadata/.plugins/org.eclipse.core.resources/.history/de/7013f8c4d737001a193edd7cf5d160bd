spring:
  datasource:
    driver-class-name: org.postgresql.Driver
    url: jdbc:postgresql://localhost:5432/db_escuela
    username: postgres
    password: 123456
  jpa:
    hibernate:
      ddl-auto: update
    show-sql: true #  me permite ver la sentencia sql que se envia    
    database-platform: org.hibernate.dialect.PostgreSQLDialect

